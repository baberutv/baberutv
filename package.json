{
  "author": "Yamagishi Kazutoshi <ykzts@desire.sh>",
  "bugs": {
    "url": "https://github.com/baberutv/baberutv/issues"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "context-provider": "^1.0.2",
    "dexie": "^v2.0.0-beta.4",
    "dialog-polyfill": "^0.4.4",
    "hls.js": "^0.7.1",
    "isomorphic-style-loader": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "url-search-params": "^0.6.1"
  },
  "description": ":tv: Television!",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-env": "^1.1.4",
    "babili-webpack-plugin": "^0.0.10",
    "copy-webpack-plugin": "^4.0.0",
    "coveralls": "^2.11.16",
    "css-loader": "^0.26.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.4.1",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.24.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.3.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^3.0.0"
  },
  "engines": {
    "node": ">= 5.0.0",
    "yarn": ">= 0.17.0"
  },
  "engineStrict": true,
  "homepage": "https://baberu.tv/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/__tests__/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "__tests__/__mocks__/"
    ],
    "testRegex": "/__tests__/.*\\.(js|jsx)$",
    "testURL": "https://baberutv.test/",
    "timers": "fake",
    "verbose": false
  },
  "license": "MIT",
  "name": "baberutv",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/baberutv/baberutv.git"
  },
  "scripts": {
    "build": "webpack --env ${NODE_ENV:-development}",
    "lint": "eslint --ext js --ext jsx .",
    "start": "webpack-dev-server",
    "test": "${npm_execpath:-npm} run lint && ${npm_execpath:-npm} run test-only",
    "test-only": "jest --coverage"
  },
  "version": "1.1.4"
}
